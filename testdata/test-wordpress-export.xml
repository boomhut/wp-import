      <content:encoded><![CDATA[<!-- wp:paragraph -->
<p>This is a <strong>test</strong> paragraph with some <em>formatting</em>.</p>
<!-- /wp:paragraph -->

<!-- wp:list {"ordered":false} -->
<ul>
  <li>Item 1</li>
  <li>Item 2</li>
  <li>Item <strong>3</strong> with formatting</li>
</ul>
<!-- /wp:list -->

<!-- wp:list {"ordered":true} -->
<ol>
  <li>First item</li>
  <li>Second item</li>
  <li>Third item with <a href="https://example.com">link</a></li>
</ol>
<!-- /wp:list -->

<!-- wp:heading -->
<h2>Sample Heading</h2>
<!-- /wp:heading -->

<!-- wp:quote -->
<blockquote class="wp-block-quote">
  <p>This is a blockquote example.</p>
</blockquote>
<!-- /wp:quote -->

<!-- wp:code -->
<pre><code>function testCode() {
  return "This is a code block";
}</code></pre>
<!-- /wp:code -->

<!-- wp:table -->
<table class="wp-block-table">
  <thead>
    <tr>
      <th>Header 1</th>
      <th>Header 2</th>
    </tr>
  </thead>
  <tbody>
    <tr>
      <td>Row 1, Cell 1</td>
      <td>Row 1, Cell 2</td>
    </tr>
    <tr>
      <td>Row 2, Cell 1</td>
      <td>Row 2, Cell 2</td>
    </tr>
  </tbody>
</table>
<!-- /wp:table -->

<!-- wp:html -->
<div class="custom-div" data-test="value">Custom HTML block</div>
<center>Centered text</center>
<font color="red">Red text</font>
<!-- /wp:html -->]]></content:encoded>
